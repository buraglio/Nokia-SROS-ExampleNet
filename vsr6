    system
        sync-if-timing
            begin
            commit
        exit
    exit
#--------------------------------------------------
echo "Management Router Configuration"
#--------------------------------------------------
    router management
    exit

#--------------------------------------------------
echo "Router (Network Side) Configuration"
#--------------------------------------------------
    router Base
        interface "system"
            address 100.64.0.6/32
            ipv6
                address 2001:db8:293:ffff::6/128
            exit
            no shutdown
        exit
        interface "vsr6-vsr1"
            address 10.2.1.22/31
            port 1/1/c1/1:5
            ipv6

                address 2001:db8:293:f3f::16/127
            exit
            no shutdown
        exit
        interface "vsr6-vsr3"
            address 10.1.1.28/31
            port 1/1/c2/1:5
            ipv6
                address 2001:db8:293:f2f::1a/127
            exit
            no shutdown
        exit
        autonomous-system 65505
        ecmp 2
        router-id 100.64.0.6
        ldp-shortcut ipv4 ipv6
#--------------------------------------------------
echo "Static Route Configuration"
#--------------------------------------------------
        static-route-entry 0.0.0.0/0
            black-hole
                shutdown
            exit
        exit
        static-route-entry ::/0
            black-hole
                shutdown
            exit
        exit
#--------------------------------------------------
echo "Aggregate Configuration"
#--------------------------------------------------
        aggregate 100.126.0.0/16
#--------------------------------------------------
echo "MPLS LABEL RANGE Configuration"
#--------------------------------------------------
        mpls-labels
            sr-labels start 400000 end 500000
        exit
#--------------------------------------------------
echo "ISIS Configuration"
#--------------------------------------------------
        isis 0
            router-id 100.64.0.6
            area-id 49.0001
            traffic-engineering
            advertise-router-capability area
            ipv6-routing mt
            multi-topology
                ipv6-unicast
            exit
            level 1
                wide-metrics-only
            exit
            level 2
                wide-metrics-only
            exit
            interface "system"
                ipv4-node-sid index 26000
                no shutdown
            exit
            interface "vsr6-vsr1"
                interface-type point-to-point
                level 1
                    metric 25000
                exit
                level 2
                    metric 25000
                exit
                no shutdown
            exit
            interface "vsr6-vsr3"
                interface-type point-to-point
                level 1
                    metric 25000
                exit
                level 2
                    metric 25000
                exit
                no shutdown
            exit
            segment-routing
                prefix-sid-range global
                no shutdown
            exit
            no shutdown
        exit
#--------------------------------------------------
echo "MPLS Configuration"
#--------------------------------------------------
        mpls
            interface "system"
                no shutdown
            exit
            interface "vsr6-vsr1"
                no shutdown
            exit
            interface "vsr6-vsr3"
                no shutdown
            exit
        exit
#--------------------------------------------------
echo "RSVP Configuration"
#--------------------------------------------------
        rsvp
            refresh-reduction-over-bypass enable
            interface "system"
                no shutdown
            exit
            interface "vsr6-vsr1"
                no shutdown
            exit
            interface "vsr6-vsr3"
                no shutdown
            exit
            no shutdown
        exit
#--------------------------------------------------
echo "MPLS LSP Configuration"
#--------------------------------------------------
        mpls
            path "loose"
                no shutdown
            exit
            no shutdown
        exit
#--------------------------------------------------
echo "LDP Configuration"
#--------------------------------------------------
        ldp
            interface-parameters
                interface "vsr6-vsr1" dual-stack
                    ipv4
                        no shutdown
                    exit
                    no shutdown
                exit
                interface "vsr6-vsr3" dual-stack
                    ipv4
                        no shutdown
                    exit
                    no shutdown
                exit
            exit
            targeted-session
            exit
            no shutdown
        exit
    exit

#--------------------------------------------------
echo "Service Configuration"
#--------------------------------------------------
    service
        customer 1 create
            description "Default customer"
        exit
        ies 1 customer 1 create
            interface "vsr6-host6" create
            exit
        exit
        ies 1 customer 1 create
            interface "vsr6-host6" create
                address 10.6.6.1/30
                ipv6
                    address 2001:db8:293:f6f::2/127
                exit
                sap 1/1/c4/1:0 create
                exit
            exit
            no shutdown
        exit
    exit
#--------------------------------------------------
echo "Router (Service Side) Configuration"
#--------------------------------------------------
    router Base
#--------------------------------------------------
echo "ISIS Configuration"
#--------------------------------------------------
        isis 0
            segment-routing
                prefix-sid-range global
                no shutdown
            exit
            no shutdown
        exit
#--------------------------------------------------
echo "Policy Configuration"
#--------------------------------------------------
        policy-options
            begin
            prefix-list "default-only"
                prefix 0.0.0.0/0 exact
            exit
            prefix-list "default6-only"
                prefix ::/0 exact
            exit
            prefix-list "vsr6-exact-v4"
                prefix 100.126.0.0/16 exact
            exit
            prefix-list "customer6-routes"
                prefix 10.66.0.0/16 exact
            exit
            prefix-list "customer6-routes6"
                prefix 2001:db8:6::/48 exact
            exit
            policy-statement "from-customer6"
                entry 10
                    from
                        prefix-list "customer6-routes"
                        family ipv4
                    exit
                    action accept
                    exit
                exit
                default-action next-policy
                exit
            exit
            policy-statement "aggregate-routes"
                entry 10
                    description "vsr6-aggregate"
                    from
                        protocol aggregate
                        prefix-list "vsr6-exact-v4"
                    exit
                    action accept
                        local-preference 10000
                    exit
                exit
            exit
            policy-statement "connected-routes"
                entry 5
                    from
                        protocol direct
                    exit
                    action accept
                        local-preference 10000
                    exit
                exit
            exit
            policy-statement "default-originate"
                entry 10
                    from
                        protocol static
                        prefix-list "default-only"
                        family ipv4
                    exit
                    action accept
                    exit
                exit
                default-action next-policy
                exit
            exit
            policy-statement "from-customer6-v6"
                entry 10
                    from
                        prefix-list "customer6-routes6"
                        family ipv6
                    exit
                    action accept
                    exit
                exit
                default-action next-policy
                exit
            exit
            policy-statement "default6-originate"
                entry 10
                    from
                        protocol static
                        prefix-list "default6-only"
                        family ipv6
                    exit
                    action accept
                    exit
                exit
                default-action next-policy
                exit
            exit
            commit
        exit
#--------------------------------------------------
echo "BGP Configuration"
#--------------------------------------------------
bgp
    ibgp-multipath
    best-path-selection
        deterministic-med
    exit
    next-hop-resolution
        shortcut-tunnel
            family ipv4
                resolution-filter
                    ldp
                    sr-isis
                exit
                resolution filter
            exit
            family ipv6
                resolution-filter
                    sr-isis
                exit
                resolution filter
            exit
        exit
        labeled-routes
            transport-tunnel
                family vpn
                    resolution-filter
                        no ldp
                        sr-isis
                        bgp
                    exit
                    resolution disabled
                exit
                family label-ipv4
                    resolution-filter
                        ldp
                        sr-isis
                    exit
                    resolution filter
                exit
                family label-ipv6
                    resolution-filter
                        ldp
                        sr-isis
                    exit
                    resolution filter
                exit
            exit
        exit
    exit
            group "ebgp"
                family ipv4 ipv6
                type external
                local-as 65505
                peer-as 65560
                neighbor 10.6.6.2
                    import "from-customer6"
                    export "default-originate"
                exit
            exit
            group "ibgp"
                family ipv4 ipv6 vpn-ipv4 mcast-ipv4 vpn-ipv6
                next-hop-self
                type internal
                export "aggregate-routes" "connected-routes"
                local-as 65505
                peer-as 65505
                local-address 100.64.0.6
                neighbor 100.64.0.1
                exit
                neighbor 100.64.0.2
                exit
                neighbor 100.64.0.3
                exit
                neighbor 100.64.0.4
                exit
                neighbor 100.64.0.5
                exit
                neighbor 100.64.0.7
                exit
            exit
            no shutdown
        exit
    exit


exit all
