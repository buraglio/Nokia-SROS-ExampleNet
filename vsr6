configure
#--------------------------------------------------
echo "System Configuration"
#--------------------------------------------------
    system
        name "vsr6"
        power-management
            mode none
        exit
        power-shelf 1
            power-shelf-type ps-a4-shelf-dc
            power-module 1
                power-module-type ps-a-dc-6000
                no shutdown
            exit
            power-module 2
                power-module-type ps-a-dc-6000
                no shutdown
