#--------------------------------------------------
echo "Router (Network Side) Configuration"
#--------------------------------------------------
    router Base
        interface "system"
            address 100.64.0.1/32
            ipv6
                address 2001:db8:293:ffff::1/128
            exit
            no shutdown
        exit
        interface "vsr1-vsr2"
            address 10.1.1.2/31
            port 1/1/c1/1:5
            ipv6
                address 2001:db8:293:f2f::2/127
            exit
            no shutdown
        exit
        interface "vsr1-vsr3"
            address 10.2.1.2/31
